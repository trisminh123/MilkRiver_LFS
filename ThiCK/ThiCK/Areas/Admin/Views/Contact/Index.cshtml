@model IEnumerable<ContactModel>
@{
	ViewData["title"] = "Contact Info";
	// Paginate pager = new Paginate();
	// int pageNo = 0;
	// if (ViewBag.Paper != null)
	// {
	// 	pager = ViewBag.Pager;
	// 	pageNo = pager.CurrentPage;
	// }
}
<h3>Contact Info</h3>
<table class="table" id="myTable">
	<thead>
		<tr>
			<th scope="col">Name</th>
			<th scope="col">Description</th>
			<th scope="col">Map</th>
			<th scope="col">Logo</th>
			<th scope="col">Email</th>
			<th scope="col">Phone</th>
			<th scope="col">Actions</th>
		</tr>
	</thead>
	<tbody>
		@foreach (var item in Model)
		{
			<tr>
				<td>@item.Name</td>
				<td>@Html.Raw(@item.Description)</td>
				<td>@Html.Raw(item.Map)</td>
				<td><img src="~/media/logo/@item.LogoImg" width="120" height="150px" /></td>
				<td>@item.Email</td>
				<td>@item.Phone</td>
				<td>
					<a asp-action="Edit" class="btn btn-warning btn-sm" asp-controller="Contact">Edit</a>
				</td>

			</tr>
		}


	</tbody>
</table>
